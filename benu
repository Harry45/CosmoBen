   "source": [
    "coords = SkyCoord(ra=ra, dec=dec, unit=\"deg\")\n",
    "\n",
    "# Convert RA to range [-180, 180]\n",
    "ra_wrapped = (coords.ra.deg + 180) % 360 - 180\n",
    "\n",
    "# Create a matplotlib figure and set Mollweide projection\n",
    "fig, ax = plt.subplots(subplot_kw={'projection': 'mollweide'})\n",
    "\n",
    "marker_size = 4\n",
    "\n",
    "# Plot the RA and DEC as points\n",
    "ax.scatter(np.radians(ra_wrapped), coords.dec.radian, color='purple', s=marker_size)  # Convert to radians for plotting\n",
    "\n",
    "# Add labels and title\n",
    "ax.set_xlabel('Right Ascension (rad)')\n",
    "ax.set_ylabel('Declination (rad)')\n",
    "ax.set_title('Sky Positions in Aitoff Projection \\n')\n",
    "\n",
    "# Optional: Add grid\n",
    "ax.grid(True)\n",
    "\n",
    "# Show the plot\n",
    "plt.show()\n"
   ]
