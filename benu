    "# Create a SkyCoord object with units in degrees\n",
    "coords = SkyCoord(ra=ra, dec=dec, unit=\"deg\")\n",
    "\n",
    "\n",
    "# Create a matplotlib figure and set Aitoff projection\n",
    "fig, ax = plt.subplots(subplot_kw={'projection': 'mollweide'})\n",
    "\n",
    "marker_size = 4\n",
    "\n",
    "\n",
    "# Plot the RA and DEC as points\n",
    "ax.scatter(coords.ra.radian, coords.dec.radian, color='purple', s=marker_size)  # Convert to radians for plotting\n",
    "\n",
    "\n",
    "\n",
    "# Add labels and title\n",
    "ax.set_xlabel('Right Ascension (rad)')\n",
    "ax.set_ylabel('Declination (rad)')\n",
    "ax.set_title('Sky Positions in Aitoff Projection \\n')\n",
    "\n",
    "# Optional: Add grid\n",
    "ax.grid(True)\n",
    "# Show the plot\n",
    "plt.show()"
   ]
